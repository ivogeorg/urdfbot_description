<?xml version="1.0"?>
<robot name="urdfbot">

  <material name="blue">
    <color rgba="0.006 0.151 0.581 1"/>
  </material>

  <material name="white">
    <color rgba="1.0 0.91 0.827 1"/>
  </material>

        
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <link name="head_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

